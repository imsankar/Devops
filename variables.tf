variable "aws_access_key" {
  description = "access key of the account"
  default     = "us-east-1"
}

variable "aws_secret_key" {
  description = "Secret key of the account"
  default     = "us-east-1"
}

variable "aws_region" {
  description = "The AWS region to create things in."
  default     = "us-east-1"
}
